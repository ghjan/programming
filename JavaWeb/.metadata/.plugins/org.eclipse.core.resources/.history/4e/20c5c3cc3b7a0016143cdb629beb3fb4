  <body  >
  		<div id="one">
  			<div id="two">
  			
  			</div>
  		</div>
  </body>
  
   <script type="text/javascript">
   //event.stopPropagation();
   //阻止事件的继续传播
   		document.getElementById("two").onclick=function (event){
   			alert("two");
   			event.stopPropagation();
   		}
   		document.getElementById("one").onclick=function (){
   			alert("one");
   		}
	</script>
