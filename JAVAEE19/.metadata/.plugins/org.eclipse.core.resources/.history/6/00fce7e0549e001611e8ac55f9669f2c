import java.util.Iterator;
import java.util.LinkedList;
import java.util.ListIterator;


public class teste {
	private static LinkedList<Integer> list;//每个男性对女性的排名
	public static void main(String[] args) {
		list = new LinkedList<Integer>();
		
		list.add(1);
		list.add(2);
		list.add(22);
		list.add(33);
		ListIterator<Integer>it=list.listIterator(0);
	/*	for(;it.hasNext();){
			it.next();
			if(it.next().equals("3")){
				break;
			}
		}*/
		for(;it.hasNext();) {
			if(it.next() >= 3 && it.next()<4)
				it.add(99);
		}	
		
		for(ListIterator<Integer>itt=list.listIterator(0);itt.hasNext();){
			System.out.println(itt.next()+"         "+itt.next());
		}
	}
}
